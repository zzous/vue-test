A;AAAA;AAAA;AAAA;AAAA;AAKP","sourcesContent":["import { call, put, takeEvery, take, fork } from 'redux-saga/effects';\r\nimport { login } from \"@/api/userApi\";\r\nimport { getUserInfo } from \"@/api/userApi\";\r\nimport { getUserList } from \"@/api/userApi\";\r\nimport * as mutations from \"./mutations\";\r\nimport * as actionTypes from \"./type\";\r\nimport {NAMESPACE} from \"./type\";\r\n\r\nimport store from \"@/store\";\r\n\r\n\r\nfunction* workLogin(params) {\r\n\r\n\ttry {\r\n\t\tlet type = mutations.G_LOGIN_PENDING;\r\n\t\tyield put({ type: type })\r\n\t\t\r\n\t\tlet success = yield call(login, params);\r\n\t\tif (success) {\t\t\t\t\r\n\t\t\tyield put({ type: mutations.G_LOGIN_SUCCESS, data: true })\t\t\t\r\n\t\t\tconsole.log(\"workLogin success\");\r\n\t\t}\r\n\t} catch (error) {\r\n\t\tyield put({ type: mutations.G_LOGIN_FAILURE, data:error })\t\t\r\n\t}\r\n\treturn false;\r\n\t//dispatch(actionTypes.GET_USER_INFO, params.userId);\r\n}\r\n\r\nfunction* workGetUserInfo(userId) {\t\r\n\ttry {\r\n\t\tyield put({type:mutations.G_USER_INFO_PENDING})\r\n\t\tlet userInfo = yield call(getUserInfo, userId);\r\n\t\tif (userInfo) {\r\n\t\t\tyield put({ type: mutations.G_USER_INFO_SUCCESS, data:userInfo })\r\n\t\t}\r\n\t\treturn true;\r\n\t} catch (error) {\r\n\t\tyield put({ type: mutations.G_USER_INFO_FAILURE, data:error })\r\n\t}\r\n\r\n\treturn false;\r\n}\r\nfunction* workGetUserList(){\r\n\ttry {\r\n\t\tyield put({type:mutations.G_USER_LIST_PENDING})\r\n\t\tlet userList = yield call(getUserList);\r\n\r\n\t\tif (userList) {\r\n\t\t\tyield put({ type: mutations.G_USER_LIST_SUCCESS, data:userList })\r\n\t\t}\r\n\t\treturn true;\r\n\t} catch (error) {\r\n\t\tyield put({ type: mutations.G_USER_LIST_FAILURE, data:error })\r\n\t}\r\n\r\n\treturn false;\r\n}\r\nfunction* test1(action) {\r\n\tconsole.log(\"AAAA\", action)\r\n\t\r\n}\r\n\r\nfunction* workLogin22() {\r\n\twhile (true) {\r\n\t\tlet params = yield take(actionTypes.ASYNC_LOGIN);\r\n\t\tconsole.log(\"params = \", params);\r\n\t}\r\n}\r\n\r\nexport function* waitUserSaga() {\r\n\tyield takeEvery(actionTypes.ASYNC_LOGIN, workLogin);\r\n\tyield takeEvery(mutations.G_LOGIN_SUCCESS, workGetUserInfo);\t\r\n}\r\n\r\n/*\r\nreact action type \r\n\r\nvue saga \r\n*/"]}]}